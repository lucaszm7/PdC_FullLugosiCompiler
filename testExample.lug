main
{
    var int x;
    var float y;
    var float z;

    var float zz;

    x := 10;
    y := 20;
    z := 30;

    zz := sqrt(z);

    if((x > 20))
    {
        y := (x + 20);
    };

    while((y > 1)) do
    {
        z := (zz + y);
        y := (y - 1);
    };

    print(z);
}

function float sqrt(float n)
{
    return (n * n);
}